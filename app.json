{
  "expo": {
    "name": "Aurashop",
    "slug": "Aurashop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://static.vecteezy.com/system/resources/thumbnails/012/986/755/small_2x/abstract-circle-logo-icon-free-png.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "https://static.vecteezy.com/system/resources/thumbnails/012/986/755/small_2x/abstract-circle-logo-icon-free-png.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "ios": {
      "bundleIdentifier": "com.itgenius.aurashop",
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let you upload profile photos.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to let you upload profile photos."
      }
    },
    "android": {
      "package": "com.itgenius.aurashop",
      "versionCode": 1,
      "ndkVersion": "26.0.10792818",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share photos with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
